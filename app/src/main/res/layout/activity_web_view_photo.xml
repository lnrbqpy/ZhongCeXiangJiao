<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_web_view_photo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="hznj.com.zhongcexiangjiao.Activity.WebViewPhotoActivity">
       <RadioGroup
           android:id="@+id/rd_btn_photo"
           android:orientation="horizontal"
           android:layout_width="500dp"
           android:layout_height="40dp"
           android:layout_gravity="center">
           <RadioButton
               android:checked="true"
               android:id="@+id/rd_btn_wentiphoto"
               android:textSize="20dp"
               android:textColor="@color/black"
               android:text="问题图片上传"
               android:layout_width="180dp"
               android:layout_height="35dp"/>

           <RadioButton
               android:layout_marginLeft="30dp"
               android:id="@+id/rd_btn_wanhaophoto"
               android:textSize="20dp"
               android:textColor="@color/black"
               android:text="修复完成后图片上传"
               android:layout_width="250dp"
               android:layout_height="35dp"/>
       </RadioGroup>
    <WebView
        android:id="@+id/maim_web"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </WebView>
</LinearLayout>
